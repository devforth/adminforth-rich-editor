{
  "name": "@adminforth/rich-editor",
  "version": "1.0.0",
  "description": "Rich editor plugin for adminforth",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepare": "npm link adminforth",
    "build": "tsc && rsync -av --exclude 'node_modules' custom dist/"
  },
  "homepage": "https://adminforth.dev/docs/tutorial/Plugins/RichEditor/",
  "repository": {
    "type": "git",
    "url": "https://github.com/devforth/adminforth-rich-editor.git"
  },
  "keywords": [
    "adminforth",
    "rich editor"
  ],
  "author": "devforth",
  "license": "ISC",
  "dependencies": {
    "adminforth": "^2.4.0-next.238",
    "cheerio": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.7",
    "semantic-release": "^24.2.1",
    "semantic-release-slack-bot": "^4.0.2",
    "typescript": "^5.7.3"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "semantic-release-slack-bot",
        {
          "notifyOnSuccess": true,
          "notifyOnFail": true,
          "slackIcon": ":package:",
          "markdownReleaseNotes": true
        }
      ]
    ],
    "branches": [
      "main",
      {
        "name": "next",
        "prerelease": true
      }
    ]
  }
}
